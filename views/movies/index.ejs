<!DOCTYPE html>
<html lang="en">
    <%- include("../partials/head.ejs") %>
<body>
    <%- include("../partials/header.ejs") %>
    <h1>Watched Movies</h1>
    <div class="movies">
        <% movies.forEach((movies, index) =>{ %>
            <img src="<%= movies.Poster %>" height="350px"></img>
            <h2 class="movie-title"><%= movies.Title %></h2>
            <h3 class="movie-date"><%= movies.Released %></h3>
            <h3 class="movie-actors"><%= movies.Actors %></h3>
            <h3 class="movie-plot"><%= movies.Plot %></h3>
        <% }) %>
    </div>
</body>
</html>